@using Models.Api.VkApi
@using ATMIT.Web.Utility;
@using Models.Afisha;
@model FirstRequest
@{
    ViewData["Title"] = "Афиша";
}

@section Styles{

}

@section Scripts {
    <script>
        appRactive.set('currentUser', @Json.Serialize(Model.ApiResult.Value.UserData));
        appRactive.set('currentUser.customData', @Json.Serialize(Model.CustomData));
        appRactive.set('categories', @Json.Serialize(Enum<PlaceType>.ToIEnumerable().ToDictionary(_x => (int)_x, _x => _x.GetDisplayName())));
    </script>
}
