{
    "version": "0.1.0",
    "command": "dotnet",
    "isShellCommand": true,
    "args": [],
    "tasks": [
        {
            "taskName": "build",
            "args": [
                "${workspaceRoot}/Web/Web.csproj"
            ],
            "isBuildCommand": true,
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "publish",
            "command": "cmd",
            "args": [
                "chcp 65001",
                "/p:DeployOnBuild=true",
                "/p:PublishProfile=Publish"
            ]
        }
    ]
}